                <script>
                   // jQuery hide()/show() example with a switch statement 

                    $(document).ready(function() {


                      // function to hide all divs 
                      function hideAll() {
                        $('#colores1').hide();
                        $('#colores2').hide();
                        $('#colores3').hide();
                        $('#colores4').hide();
                        $('#colores5').hide();
                        $('#colores6').hide();
                        $('#colores7').hide();
                        $('#colores8').hide();
						$('#colores9').hide();
						$('#colores10').hide();
						$('#colores11').hide();
						$('#colores12').hide();
						$('#colores13').hide();
						$('#colores14').hide();
						  $('#colores15').hide();
						$('#colores16').hide();
						  
						  $('#colores17').hide();
						  $('#colores18').hide();
						  $('#colores19').hide();
						  $('#colores20').hide();
						  $('#colores21').hide();
						  $('#colores22').hide();
						  
                        
                      }

                      // run that function right away
                      hideAll();

                      // when any animal is clicked, make the suitable div appear
                      $('.animalIcon').click(function() {

                        // but first, hide all the divs to ensure that 
                        // only one will be open, ever 
                        hideAll();

                        // here is a switch statement - this was in Codecademy 
                        // "Control Flow" lesson 7

                        // we can get the ID of the thing that was clicked - 
                        switch ($(this).attr("id")) {
                          case "material1":
                            $('#colores1').show();
                            break;
                          case "material2":
                            $('#colores2').show();
                            break;
                        case "material3":
                            $('#colores3').show();
                            break;
                                case "material4":
                            $('#colores4').show();
                            break;
                                case "material5":
                            $('#colores5').show();
                            break;
                                case "material6":
                            $('#colores6').show();
                            break;
                                case "material7":
                            $('#colores7').show();
                            break;
                                case "material8":
                            $('#colores8').show();
                            break;
								case "material9":
                            $('#colores9').show();
                            break;
								case "material10":
                            $('#colores10').show();
                            break;
								case "material11":
                            $('#colores11').show();
                            break;
								case "material12":
                            $('#colores12').show();
                            break;
								case "material13":
                            $('#colores13').show();
                            break;
								case "material14":
                            $('#colores14').show();
                            break;
								case "material15":
                            $('#colores15').show();
                            break;
								case "material16":
                            $('#colores16').show();
                            break;
								case "material17":
                            $('#colores17').show();
                            break;
								case "material18":
                            $('#colores18').show();
                            break;
								case "material19":
                            $('#colores19').show();
                            break;
								case "material20":
                            $('#colores20').show();
                            break;
								case "material21":
                            $('#colores21').show();
                            break;
								case "material22":
                            $('#colores22').show();
                            break;
                          
                        }
                      }); // end of function for clicking 


                    });
                   
                   
                    
                </script>

<?php } ?>
